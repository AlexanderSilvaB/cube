import lists as default

func str.len()
{
    return len(this)
}

func num.sig()
{
    if(this > 0)
        return 1
    return -1
}

var s = "ABC"
println(s,".len() = ", s.len())

println(5, ".sig() = ", 5.sig())
println(-5, ".sig() = ", -5.sig())

var data = 1..10

data = data.filter(@(x) => x > 4)

println(data)